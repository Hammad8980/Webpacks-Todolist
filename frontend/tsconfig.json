{
  "compilerOptions": {
    "target": "es2020",
    "module": "esnext",
    "lib": ["ES2020", "ES2015", "DOM", "DOM.Iterable"],
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noEmit": false
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist", "webpack.*.js", "*.config.js", ".storybook"]
}
